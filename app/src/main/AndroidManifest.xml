<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.acuit.jointdistribution">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name="com.acuit.jointdistribution.Common.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name="com.acuit.jointdistribution.Supplier.Activity.HomeActivity"/>
        <activity android:name="com.acuit.jointdistribution.Supplier.Activity.ReceivedActivity" />
        <activity android:name="com.acuit.jointdistribution.Supplier.Activity.MenuActivity" />
        <activity android:name="com.acuit.jointdistribution.Supplier.Activity.MenuInfoActivity" />
        <activity android:name="com.acuit.jointdistribution.Supplier.Activity.SendActivity" />
        <activity android:name="com.acuit.jointdistribution.Supplier.Activity.SendInfoActivity" />
        <activity android:name="com.acuit.jointdistribution.Supplier.Activity.DistributionActivity" />
        <activity android:name="com.acuit.jointdistribution.Supplier.Activity.CommandActivity" />
        <activity android:name="com.acuit.jointdistribution.Supplier.Activity.BusinessOrderActivity"></activity>
    </application>

</manifest>