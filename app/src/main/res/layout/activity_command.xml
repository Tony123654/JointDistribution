<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.acuit.jointdistribution.Supplier.Acitivity.CommandActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#23b587"
        android:id="@+id/relativeLayout4"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:textSize="24sp"
            android:text="我的评论"
            android:id="@+id/textView" />
        <ImageButton
            android:id="@+id/ib_back"
            android:background="#00000000"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@mipmap/icon_nav_back"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_alignTop="@+id/textView"
            android:layout_alignParentStart="true"
            android:layout_marginStart="8dp"/>


    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:id="@+id/relativeLayout5"
        android:layout_below="@+id/relativeLayout4"
        android:layout_alignParentStart="true">

        <ImageButton
            android:id="@+id/imageButton"
            android:background="#00000000"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="50dp"
            app:srcCompat="@mipmap/start"
            android:layout_above="@+id/textView9"
            android:layout_alignParentStart="true" />

        <ImageButton
            android:id="@+id/imageButton2"
            android:background="#00000000"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/imageButton"
            android:layout_marginStart="19dp"
            android:layout_toEndOf="@+id/imageButton"
            app:srcCompat="@mipmap/start" />
        <ImageButton
            android:id="@+id/imageButton3"
            android:background="#00000000"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            app:srcCompat="@mipmap/start"
            android:layout_alignTop="@id/imageButton2"
            android:layout_toEndOf="@id/imageButton2" />

        <ImageButton
            android:id="@+id/imageButton4"
            android:background="#00000000"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@id/imageButton3"
            android:layout_marginStart="24dp"
            android:layout_toEndOf="@id/imageButton3"
            app:srcCompat="@mipmap/start" />

        <ImageButton
            android:id="@+id/imageButton5"
            android:background="#00000000"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@id/imageButton4"
            android:layout_marginStart="18dp"
            android:layout_toEndOf="@id/imageButton4"
            app:srcCompat="@mipmap/huixingxing" />
        <TextView
            android:layout_margin="8dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
             android:textSize="24sp"
            android:text="综合3.0分"
            android:id="@id/textView9" />


    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_below="@+id/relativeLayout5">

    </RelativeLayout>


</RelativeLayout>
